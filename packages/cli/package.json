{
	"name": "@postgresql-typed/cli",
	"version": "0.2.0",
	"description": "Generate TypeScript types to be used with the @postgresql-typed ecosystem from your PostgreSQL database.",
	"type": "module",
	"exports": {
		".": "./lib/index.js",
		"./config": "./lib/config.js"
	},
	"types": "./lib/index.d.ts",
	"license": "MPL-2.0",
	"author": {
		"name": "Bas950",
		"email": "me@bas950.com",
		"url": "https://bas950.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PostgreSQL-Typed/PostgreSQL-Typed.git",
		"directory": "packages/cli"
	},
	"bugs": {
		"url": "https://github.com/PostgreSQL-Typed/PostgreSQL-Typed/issues"
	},
	"files": [
		"lib"
	],
	"bin": {
		"pgt": "./lib/cli.js"
	},
	"keywords": [
		"postgresql",
		"pg",
		"typed",
		"types",
		"automatic",
		"type-safe",
		"generator",
		"generate",
		"typescript",
		"cli"
	],
	"scripts": {
		"dev": "tsc -w",
		"build": "tsc",
		"lint": "eslint --fix --ext .ts ./ && prettier -w .",
		"lint:ci": "eslint --ext .ts ./ && prettier --check ."
	},
	"dependencies": {
		"@postgresql-typed/oids": "workspace:*",
		"@postgresql-typed/util": "workspace:*",
		"camelcase": "6.3.0",
		"chalk": "4.1.2",
		"command-line-args": "^5.2.1",
		"command-line-commands": "^3.0.2",
		"command-line-usage": "^6.1.3",
		"cosmiconfig": "^8.0.0",
		"cosmiconfig-typescript-loader": "^4.3.0",
		"debug": "^4.3.4",
		"mkdirp": "^1.0.4",
		"pascalcase": "1.0.0",
		"pg": "^8.7.3",
		"pluralize": "^8.0.0",
		"source-map-support": "^0.5.21",
		"ts-node": "^10.9.1",
		"zod": "^3.18.0"
	},
	"devDependencies": {
		"@types/command-line-args": "^5.2.0",
		"@types/command-line-commands": "^2.0.1",
		"@types/command-line-usage": "^5.0.2",
		"@types/debug": "^4.1.7",
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^18.6.2",
		"@types/pascalcase": "^1.0.1",
		"@types/pg": "^8.6.5",
		"@types/pluralize": "^0.0.29",
		"typescript": "^4.7.4"
	},
	"peerDependencies": {
		"@postgresql-typed/ui": "workspace:*"
	},
	"peerDependenciesMeta": {
		"@postgresql-typed/ui": {
			"optional": true
		}
	},
	"packageManager": "pnpm@7.17.0",
	"engines": {
		"node": ">=16",
		"pnpm": ">=7.17.0"
	}
}
