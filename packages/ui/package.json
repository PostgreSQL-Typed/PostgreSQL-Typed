{
	"private": true,
	"name": "@postgresql-typed/ui",
	"version": "1.0.0",
	"description": "Visualize your PostgreSQL database.",
	"license": "MPL-2.0",
	"author": {
		"name": "Bas950",
		"email": "me@bas950.com",
		"url": "https://bas950.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PostgreSQL-Typed/PostgreSQL-Typed.git",
		"directory": "packages/ui"
	},
	"bugs": {
		"url": "https://github.com/PostgreSQL-Typed/PostgreSQL-Typed/issues"
	},
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check build-only",
		"preview": "vite preview",
		"test:unit": "vitest",
		"test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
		"test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
		"test:ci": "echo 'test'",
		"build-only": "vite build",
		"type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
		"lint": "eslint --fix --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts ./ && prettier -w .",
		"lint:ci": "eslint --ext .vue,.js,.cjs,.mjs,.ts,.cts,.mts ./ && prettier --check ."
	},
	"devDependencies": {
		"@iconify-json/carbon": "^1.1.16",
		"@postgresql-typed/cli": "workspace:*",
		"@rushstack/eslint-patch": "^1.2.0",
		"@types/codemirror": "^5.60.6",
		"@types/d3": "^7.4.0",
		"@types/jsdom": "^21.1.0",
		"@types/node": "^18.14.2",
		"@types/splitpanes": "^2.2.1",
		"@unocss/eslint-config": "^0.50.6",
		"@unocss/preset-icons": "^0.50.6",
		"@unocss/reset": "^0.50.6",
		"@vitejs/plugin-vue": "^4.0.0",
		"@vue/eslint-config-prettier": "^7.1.0",
		"@vue/eslint-config-typescript": "^11.0.2",
		"@vue/test-utils": "^2.3.0",
		"@vue/tsconfig": "^0.1.3",
		"@vueuse/core": "^9.13.0",
		"codemirror": "^5.65.11",
		"codemirror-theme-vars": "^0.1.1",
		"cypress": "^12.7.0",
		"d3": "^7.8.2",
		"d3-graph-controller": "^2.5.2",
		"eslint": "^8.34.0",
		"eslint-plugin-cypress": "^2.12.1",
		"eslint-plugin-vue": "^9.9.0",
		"floating-vue": "2.0.0-beta.20",
		"jsdom": "^21.1.0",
		"ky": "^0.33.3",
		"npm-run-all": "^4.1.5",
		"pathe": "^1.1.0",
		"prettier": "^2.8.4",
		"pretty-bytes": "^6.1.0",
		"sass": "^1.59.3",
		"splitpanes": "^3.1.5",
		"start-server-and-test": "^2.0.0",
		"typescript": "~4.8.4",
		"unocss": "^0.50.6",
		"unplugin-auto-import": "^0.15.1",
		"unplugin-vue-components": "^0.24.1",
		"vite": "^4.1.4",
		"vitest": "^0.29.1",
		"vue": "^3.2.47",
		"vue-tsc": "^1.2.0"
	},
	"packageManager": "pnpm@7.17.0"
}
