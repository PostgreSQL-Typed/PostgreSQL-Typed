{
	"name": "postgresql-types-generator",
	"version": "1.0.0",
	"description": "Automatically generated TypeScript types from your PostgreSQL database",
	"main": "lib/index.js",
	"types": "lib/index.d.ts",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PostgreSQL-Typed/PostgreSQL-Typed.git"
	},
	"bugs": {
		"url": "https://github.com/PostgreSQL-Typed/PostgreSQL-Typed/issues"
	},
	"files": [
		"lib"
	],
	"bin": {
		"pgtg": "lib/cli.js"
	},
	"scripts": {
		"dev": "tsc -w",
		"build": "tsc",
		"lint": "eslint --fix --ext .ts ./ && prettier -w .",
		"lint:ci": "eslint --ext .ts ./ && prettier --check ."
	},
	"keywords": [
		"postgresql",
		"pg",
		"typed",
		"types",
		"automatic",
		"type-safe",
		"generator",
		"generate",
		"typescript"
	],
	"author": "Bas950",
	"license": "MPL-2.0",
	"devDependencies": {
		"@types/command-line-args": "^5.2.0",
		"@types/command-line-commands": "^2.0.1",
		"@types/command-line-usage": "^5.0.2",
		"@types/debug": "^4.1.7",
		"@types/mkdirp": "^1.0.2",
		"@types/node": "^18.6.2",
		"@types/pascalcase": "^1.0.1",
		"@types/pg": "^8.6.5",
		"@types/pluralize": "^0.0.29",
		"source-map-support": "^0.5.21",
		"typescript": "^4.7.4"
	},
	"dependencies": {
		"camelcase": "6.3.0",
		"chalk": "4.1.2",
		"command-line-args": "^5.2.1",
		"command-line-commands": "^3.0.2",
		"command-line-usage": "^6.1.3",
		"cosmiconfig": "^7.0.1",
		"cosmiconfig-typescript-loader": "^3.1.1",
		"debug": "^4.3.4",
		"mkdirp": "^1.0.4",
		"pascalcase": "1.0.0",
		"pg": "^8.7.3",
		"pluralize": "^8.0.0",
		"postgresql-data-types": "workspace:*",
		"ts-node": "^10.9.1",
		"zod": "^3.18.0"
	},
	"peerDependencies": {
		"postgresql-typed": "workspace:*",
		"zod": "^3.18.0"
	},
	"peerDependenciesMeta": {
		"zod": {
			"optional": true
		}
	}
}
