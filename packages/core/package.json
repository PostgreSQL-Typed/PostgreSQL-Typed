{
	"name": "@postgresql-typed/core",
	"version": "0.14.0",
	"description": "A type-safe PostgreSQL client for Node.js",
	"type": "module",
	"exports": {
		".": "./lib/index.js",
		"./definers": {
			"types": "./definers.d.ts",
			"import": "./definers.js"
		},
		"./operators": {
			"types": "./operators.d.ts",
			"import": "./operators.js"
		},
		"./drizzle-orm": {
			"types": "./drizzle-orm.d.ts",
			"import": "./drizzle-orm.js"
		},
		"./config": {
			"types": "./config.d.ts",
			"import": "./config.js"
		},
		"./package.json": "./package.json"
	},
	"types": "./lib/index.d.ts",
	"license": "MPL-2.0",
	"author": {
		"name": "Bas950",
		"email": "me@bas950.com",
		"url": "https://bas950.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PostgreSQL-Typed/PostgreSQL-Typed.git",
		"directory": "packages/core"
	},
	"bugs": {
		"url": "https://github.com/PostgreSQL-Typed/PostgreSQL-Typed/issues"
	},
	"files": [
		"lib",
		"definers.*",
		"operators.*",
		"drizzle-orm.*",
		"config.*"
	],
	"keywords": [
		"postgresql",
		"pg",
		"typed",
		"types",
		"typescript"
	],
	"scripts": {
		"dev": "tsc -w",
		"build": "tsc",
		"lint": "eslint --ext .ts ./ && prettier --check .",
		"lint:fix": "eslint --fix --ext .ts ./ && prettier -w .",
		"test": "TZ=UTC vitest --run",
		"test:watch": "TZ=UTC vitest --ui --coverage",
		"test:ci": "TZ=UTC vitest --run --coverage"
	},
	"dependencies": {
		"@postgresql-typed/oids": "workspace:*",
		"@postgresql-typed/parsers": "workspace:*",
		"@postgresql-typed/util": "workspace:*",
		"@types/pg": "^8.10.7",
		"c12": "^1.5.1",
		"drizzle-orm": "0.30.10",
		"hookable": "^5.5.3",
		"jiti": "^1.21.0",
		"mlly": "^1.4.2",
		"pathe": "^1.1.1",
		"pg": "^8.11.3",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@postgresql-typed/cli": "workspace:*",
		"@types/node": "^20.8.10",
		"typescript": "^5.2.2"
	},
	"publishConfig": {
		"provenance": true
	},
	"packageManager": "pnpm@8.9.0",
	"engines": {
		"node": ">=16",
		"pnpm": ">=8.6.0"
	}
}
