{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["lib/**/*", "dist/**/*", ".output/**/*", ".nuxt/**/*"]
		},
		"lint": {},
		"lint:ci": {},
		"test": {
			"dependsOn": ["lint:ci"],
			"outputs": ["coverage/**/*"]
		},
		"test:watch": {
			"cache": false
		},
		"test:ci": {
			"outputs": ["coverage/**/*"]
		}
	},
	"globalDependencies": ["tsconfig.json", "vitest.config.ts", ".eslintrc"]
}
