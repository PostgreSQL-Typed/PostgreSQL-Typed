{
	"name": "@postgresql-typed/cli",
	"version": "0.3.2",
	"description": "Generate TypeScript types to be used with the PostgreSQL-Typed ecosystem from your PostgreSQL database.",
	"type": "module",
	"types": "./lib/index.d.ts",
	"bin": {
		"pgt": "./lib/cli.js"
	},
	"exports": {
		".": "./lib/index.js",
		"./config": {
			"types": "./config.d.ts",
			"import": "./config.js",
			"require": "./config.cjs"
		},
		"./package.json": "./package.json"
	},
	"files": [
		"lib",
		"config.*"
	],
	"license": "MPL-2.0",
	"author": {
		"name": "Bas950",
		"email": "me@bas950.com",
		"url": "https://bas950.com"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PostgreSQL-Typed/PostgreSQL-Typed.git",
		"directory": "packages/cli"
	},
	"bugs": {
		"url": "https://github.com/PostgreSQL-Typed/PostgreSQL-Typed/issues"
	},
	"keywords": [
		"postgresql",
		"pg",
		"typed",
		"types",
		"automatic",
		"type-safe",
		"generator",
		"generate",
		"typescript",
		"cli"
	],
	"scripts": {
		"dev": "tsc -w",
		"build": "tsc",
		"lint": "eslint --fix --ext .ts ./ && prettier -w .",
		"lint:ci": "eslint --ext .ts ./ && prettier --check ."
	},
	"dependencies": {
		"@postgresql-typed/oids": "workspace:*",
		"@postgresql-typed/util": "workspace:*",
		"c12": "^1.4.1",
		"camelcase": "7.0.1",
		"chalk": "5.2.0",
		"command-line-args": "^5.2.1",
		"command-line-commands": "^3.0.2",
		"command-line-usage": "^7.0.1",
		"debug": "^4.3.4",
		"mkdirp": "^3.0.1",
		"pascalcase": "2.0.0",
		"pathe": "^1.1.0",
		"pg": "^8.10.0",
		"pluralize": "^8.0.0",
		"source-map-support": "^0.5.21"
	},
	"devDependencies": {
		"@types/command-line-args": "^5.2.0",
		"@types/command-line-commands": "^2.0.1",
		"@types/command-line-usage": "^5.0.2",
		"@types/debug": "^4.1.7",
		"@types/node": "^20.1.1",
		"@types/pascalcase": "^1.0.1",
		"@types/pg": "^8.6.6",
		"@types/pluralize": "^0.0.29",
		"typescript": "^4.7.4"
	},
	"packageManager": "pnpm@7.17.0",
	"engines": {
		"node": ">=16",
		"pnpm": ">=7.17.0"
	}
}
